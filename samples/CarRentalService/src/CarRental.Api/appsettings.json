{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "Chatter": {
    "ServiceBus": {
      "ConnectionString": "",
      "TransportType": 1
    },
    "MessageBrokers": {
      "Reliability": {
        "RouteMessagesToOutbox": true,
        "MinutesToLiveInMemory": 0.5,
        "OutboxProcessingIntervalInMilliseconds": 60000,
        "EnableOutboxPollingProcessor":  true
      }
    }
  }
}
